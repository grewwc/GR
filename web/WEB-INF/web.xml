<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>CoordInput</servlet-name>
        <servlet-class>input.g_input.CoordInput</servlet-class>
        <init-param>
            <param-name>projectDir</param-name>
            <param-value>C:/Users/User/IdeaProjects/GR</param-value>
        </init-param>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>CoordInput</servlet-name>
        <url-pattern>/process_coord_input</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>ProecssInputServlet</servlet-name>
        <servlet-class>input.g_input.ProcessInputServlet</servlet-class>
        <init-param>
            <param-name>projectDir</param-name>
            <param-value>C:/Users/User/IdeaProjects/GR</param-value>
        </init-param>
    </servlet>

    <servlet-mapping>
        <servlet-name>ProecssInputServlet</servlet-name>
        <url-pattern>/result</url-pattern>
    </servlet-mapping>
</web-app>






